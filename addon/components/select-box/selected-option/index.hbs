<div
  class="select-box__selected-option"
  id={{this.id}}
  {{!-- begin issue: https://github.com/emberjs/rfcs/issues/497 --}}
  {{! template-lint-disable no-invalid-interactive no-positive-tabindex }}
  role={{@role}}
  onclick={{@onclick}}
  onmousedown={{@onmousedown}}
  tabindex={{@tabindex}}
  {{!-- end issue --}}
  ...attributes
  {{did-insert this.handleInsertElement}}
  {{will-destroy this.handleDestroyElement}}
>
  {{yield}}
</div>