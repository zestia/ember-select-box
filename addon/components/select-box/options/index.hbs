<div
  class="select-box__options"
  id={{this.id}}
  role={{if @selectBox.isCombobox "listbox"}}
  {{!-- begin issue: https://github.com/emberjs/rfcs/issues/497 --}}
  {{! template-lint-disable no-invalid-interactive }}
  onclick={{@onclick}}
  {{!-- end issue --}}
  ...attributes
  {{did-insert this.handleInsertElement}}
  {{will-destroy this.handleDestroyElement}}
>
  {{yield}}
</div>