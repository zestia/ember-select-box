{
  "name": "@zestia/ember-select-box",
  "version": "7.2.2",
  "description": "A faux select box for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint --allow-empty-input 'addon/**/*.{css,scss}' && stylelint --allow-empty-input 'app/**/*.{css,scss}'",
    "start": "ember serve",
    "test": "ember test && yarn lint",
    "test:all": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-select-box.git"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@zestia/ember-template-lint-plugin": "2.0.1",
    "@zestia/eslint-config": "2.0.14",
    "@zestia/prettier-config": "0.0.3",
    "@zestia/stylelint-config": "2.0.10",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli-dependency-checker": "3.1.0",
    "ember-cli-github-pages": "0.2.1",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-cli-update": "0.33.1",
    "ember-cli": "3.9.0",
    "ember-data": "3.9.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-factory-for-polyfill": "1.3.1",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.4.1",
    "ember-resolver": "5.1.3",
    "ember-source-channel-url": "1.1.0",
    "ember-source": "3.9.1",
    "ember-template-lint": "1.1.0",
    "ember-try": "1.1.0",
    "eslint": "5.16.0",
    "loader.js": "4.7.0",
    "prettier": "1.17.0",
    "qunit-dom": "0.8.4",
    "sass": "1.17.4",
    "stylelint": "10.0.1"
  },
  "dependencies": {
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-angle-bracket-invocation-polyfill": "^1.3.0",
    "ember-named-arguments-polyfill": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "select",
    "select-box",
    "dropdown"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-select-box",
    "versionCompatibility": {
      "ember": ">=2.12.0 <=canary"
    }
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-select-box/issues"
  },
  "homepage": "https://github.com/zestia/ember-select-box",
  "configPath": "tests/dummy/config"
}
