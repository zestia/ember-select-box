    "configPath" : "tests/dummy/config",
    "demoURL" : "https://zestia.github.io/ember-select-box"
  },
  "engines" : {
    "node" : "14.* || >= 16"
  },
  "keywords" : [
    "ember-addon",
    "select",
    "select-box",
    "autocomplete",
    "dropdown",
    "menu"
