{
  "author": "Zestia",
  "dependencies": {
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/util": "^1.11.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-concurrency": "^3.0.0",
    "tracked-built-ins": "^3.1.1"
  },
  "description": "A faux select box for Ember apps",
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@zestia/ember-auto-focus": "^5.1.0",
    "@zestia/eslint-config": "^5.1.1",
    "@zestia/prettier-config": "^1.0.10",
    "@zestia/stylelint-config": "^5.0.0",
    "@zestia/template-lint-config": "^6.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-cli": "^5.0.0",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^5.1.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.1",
    "ember-source": "~5.1.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-n": "^16.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.11.0",
    "sass": "^1.63.6",
    "stylelint": "^15.9.0",
    "webpack": "^5.88.1"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-select-box"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "keywords": [
    "ember-addon",
    "select",
    "select-box",
    "autocomplete",
    "dropdown",
    "menu"
  ],
  "license": "MIT",
  "name": "@zestia/ember-select-box",
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-select-box.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prettier": "prettier . --write",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently --kill-others-on-fail \"npm:lint\" \"npm:test:ember\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "version": "16.0.3"
}
